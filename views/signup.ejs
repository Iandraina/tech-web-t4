<div id="form">
    <p>Login form</p>
    <hr/>
    <form action="/signup" method="post">
        <div class="form-group">
            <label for="form_first_name">First name</label>
            <input id="form_first_name" type="text" name="first_name" />
          </div>
          <div class="form-group">
            <label for="form_name">Name</label>
            <input id="form_name" type="text" name="name" />
          </div>
      <div class="form-group">
        <label for="form_username">Username</label>
        <input id="form_username" type="text" name="username" />
      </div>
      <div class="form-group">
        <label for="form_password">Password</label>
        <input id="form_password" type="password" name="password" />
      </div>
      <button class="btn btn-primary btn-block" type="submit" value="Submit">Create account</button>
    </form>
    <hr/>
  </div>
  